project main_E1_semi2WF_T50 is

    for Languages use ("ada");
    for Main use ("main_E1_semi2WF_T50.adb");
    for Source_Dirs use ("src", "../../../common");
    for Object_Dir use "obj";
    for Runtime ("ada") use "ciccio";
    for Target use "arm-eabi";

    package Compiler is
        for Switches ("ada") use ("-g", "-gnatwa", "-gnatQ");
    end Compiler;

    package Builder is
        for Switches ("ada") use ("-g", "-O0");
    end Builder;

end main_E1_semi2WF_T50;